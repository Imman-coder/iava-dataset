	<script type="text/javascript">
		//If in Datepicker Same Date Selected in 2 times showing Error By using this function Error Rectified
		function DataPickerShowHide(id){
			$(id).on('show', function(e){
			    if ( e.date)  {
			         $(this).data('stickyDate', e.date);
			    }
			    else {
			         $(this).data('stickyDate', null);
			    }
			});
			$(id).on('hide', function(e){
			    var stickyDate = $(this).data('stickyDate');

			    if ( !e.date && stickyDate)  {
			        $(this).datepicker('setDate', stickyDate);
			        $(this).data('stickyDate', null);
			    }
			});
		}
		$.ajax({
		    type: "Get",
		    url: "../include/configLebelText.json",
		    dataType: "json",
		    success: function(data) {
				$.each(data.items, function(key, val) {  
				   $('.nav>li>a:contains("'+val.old+'"),th:contains("'+val.old+'"),label:contains("'+val.old+'"),h4:contains("'+val.old+'")').each(function() {
				   		$(this).html($(this).html().replace(val.old, val.new).replace((val.old).toUpperCase(), (val.new).toUpperCase()));
					}); 
				   $('.nav>li>a:contains("'+(val.old).toUpperCase()+'"),th:contains("'+(val.old).toUpperCase()+'"),label:contains("'+(val.old).toUpperCase()+'"),h4:contains("'+(val.old).toUpperCase()+'")').each(function() {
				   		$(this).html($(this).html().replace(val.old, val.new).replace((val.old).toUpperCase(), (val.new).toUpperCase()));
					}); 
				})
		    },error: function(){ 
		        alert("json not found"); 
		    }
		});
		/*var tab_name = $('ul li a').text();
		console.log(tab_name);
		var new_tab_name = "Stream";
		if(tab_name == new_tab_name){
			$('ul.nav nav-tabs li a').text("Program Level");
			
		}*/
	</script>
	
	


